# Keenetic router credentials (required)
KEENETIC_LOGIN=admin
KEENETIC_PASSWORD=your_router_password
KEENETIC_HOST=192.168.0.1

# VPN interfaces (comma-separated for multiple)
KEENETIC_VPN_INTERFACES=Wireguard0

# Domains configuration
DOMAINS_FILE=./config/domains.yml
DOMAINS_MASK=32

# DNS settings
DNS_SERVERS=1.1.1.1,8.8.8.8

# DNS server base URL (schema+host, e.g., http://dns.internal:8080)
# Used for DNS log monitoring and route generation
DNS_SERVER_URL=http://dns.internal:8080

# Operational settings
MINIMIZE=false
DELETE_ROUTES=true
LOG_LEVEL=INFO

# Optional overrides
GITHUB_META_URL=https://api.github.com/meta
COOKIE_FILE=config/cookie
REQUEST_DUMPS_DIR=tmp/request-dumps
